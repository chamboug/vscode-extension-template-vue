{
    "name": "vscode-extension-template-vue",
    "version": "0.1.0",
    "private": true,
    "engines": {
        "vscode": "^1.40.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./src-extension/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "myExtension.start",
                "title": "My Extension: Start"
            }
        ]
    },
    "scripts": {
        "postinstall": "npm run build",
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint"
    },
    "dependencies": {
        "vue": "^2.6.12",
        "vuetify": "^2.6.10"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^4.5.11",
        "@vue/cli-plugin-eslint": "^4.5.11",
        "@vue/cli-service": "^4.5.11",
        "babel-eslint": "^10.1.0",
        "core-js": "^3.8.3",
        "eslint": "^7.19.0",
        "eslint-plugin-vue": "^7.5.0",
        "html-webpack-inline-source-plugin": "0.0.10",
        "html-webpack-plugin": "^4.5.1",
        "sass": "^1.32.6",
        "sass-loader": "^8.0.0",
        "vue-cli-plugin-vuetify": "^2.1.0",
        "vue-template-compiler": "^2.6.12",
        "vuetify-loader": "^1.7.2"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "rules": {},
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ]
}
